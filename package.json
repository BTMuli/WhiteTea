{
  "name": "white-tea",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.9.0",
  "description": "A Probot app",
  "author": "BTMuli",
  "license": "ISC",
  "homepage": "https://github.com/BTMuli/WhiteTea",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "start": "ts-node src/index.ts",
    "still": "forever -c \"node --loader ts-node/esm ./src/index.ts\" ./",
    "lint:code": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "probot": "^12.3.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.6",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.0.3",
    "smee-client": "^1.2.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}

{
  "name": "white-tea",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.0.25",
  "description": "A Probot app",
  "author": "BTMuli",
  "license": "ISC",
  "homepage": "https://github.com/BTMuli/WhiteTea",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "start": "sh src/scripts/start.sh",
    "stop": "sh src/scripts/stop.sh",
    "reload": "sh src/scripts/reload.sh",
    "forever": "forever list",
    "dev": "nodemon --watch src --exec bun ./src/index.ts",
    "clean": "bun ./src/utils/clean.ts",
    "lint:code": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "prettier": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "log4js": "^6.9.1",
    "probot": "^13.0.2",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "smee-client": "^2.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">= 10.13.0"
  }
}
